{
    "extends": [
        "react-app",
        "react-app/jest"
    ],
    "rules": {
        "no-console": "warn",
        "prefer-const": "error",
        "quotes": ["error", "double"],
        "max-len": ["error", {"code": 140}],
        "comma-dangle": ["error", "always-multiline"],
        "semi": ["error", "always"],
        "react-hooks/exhaustive-deps": "error"
    }
}