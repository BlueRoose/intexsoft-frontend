{
    "extends": [
        "react-app",
        "react-app/jest"
    ],
    "rules": {
        "no-console": "warn",
        "prefer-const": "warn",
        "quotes": ["warn", "double"],
        "max-len": ["warn", {"code": 120}],
        "comma-dangle": ["warn", "always-multiline"],
        "semi": ["warn", "always"]
    }
}